{
  "name": "eslint-config-tron",
  "version": "1.0.6",
  "description": "Tron's ESLint config with TypeScript, Hooks and Prettier support",
  "author": "Nicolas Trote aka Tron",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nicolastrote/eslint-config-tron.git"
  },
  "bugs": {
    "url": "https://github.com/nicolastrote/eslint-config-tron/issues"
  },
  "homepage": "https://github.com/nicolastrote/eslint-config-tron#readme",
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "eslint",
    "es2018",
    "eslintconfig",
    "tron",
    "hooks",
    "typescript",
    "prettier"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "test": "eslint --ext .tsx,ts test/",
    "release": "npm test && standard-version",
    "release:dryrun": "standard-version --dry-run"
  },
  "dependencies": {
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^7.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@types/jest": "^26.0.15",
    "@types/node": "14.14.25",
    "@types/react-dom": "^17.0.0",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "7.19.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-react": "7.22.0",
    "jest": "^26.6.0",
    "prettier": "^2.2.1",
    "react-dom": "^17.0.1",
    "react": "^17.0.1",
    "standard-version": "^9.1.1",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "eslint-plugin-prettier": "^3.3.1",
    "eslint": "7.19.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.2"
  },
  "directories": {
    "test": "test"
  }
}