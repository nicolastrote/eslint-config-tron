{
  "name": "eslint-config-tron-typescript-hooks-prettier",
  "version": "1.0.0",
  "description": "Tron's ESLint config with TypeScript, Hooks and Prettier support",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "test": "eslint --ext .tsx,ts test/",
    "release": "npm test && standard-version",
    "release:dryrun": "standard-version --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nicolastrote/eslint-config-tron-typescript-hooks-prettier.git"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "eslint",
    "tron",
    "hooks",
    "typescript",
    "prettier"
  ],
  "author": "Nicolas Trote",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nicolastrote/eslint-config-tron-typescript-hooks-prettier/issues"
  },
  "homepage": "https://github.com/nicolastrote/eslint-config-tron-typescript-hooks-prettier#readme",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1"
  },
  "peerDependencies": {
    "eslint": "^7.21.0",
    "prettier": "^1.18.2 || ^2.0.0",
    "typescript": "^4.0.7"
  },
  "devDependencies": {
    "@types/react": "^17.0.2",
    "prettier": "2.1.2",
    "react": "^17.0.1",
